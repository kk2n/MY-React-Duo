{
  "presets": ["react", "es2015","stage-0"],
  "env": {
    "development": {
      "plugins": [
        "transform-decorators-legacy",
        ["import", { "libraryName": "antd", "style": true }],
        ["react-transform", {"transforms":
          [{
            "transform": "react-transform-hmr",
            "imports": ["react"],
            "locals": ["module"]
          }]
        }]
      ]
    }
  }
}